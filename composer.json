{
    "name": "zerai/openswoole-runtime",
    "type": "library",
    "description": "A openswoole runtime component.",
    "keywords": [
        "openswoole",
        "symfony runtime"
    ],
    "homepage": "https://github.com/zerai/openswoole-runtime",
    "version": "0.1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.5",
        "ext-openswoole": "^22.0",
        "symfony/runtime": "^5.4 || ^6.0",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "illuminate/http": "^9.14",
        "openswoole/ide-helper": "^22.0",
        "phpunit/phpunit": "^9.5",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "tools"
        }
    },
    "autoload": {
        "psr-4": {
            "Zerai\\OpenSwoole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zerai\\OpenSwoole\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
